<Marisa>

	<append xpath="/Sounds">

		<SoundDataNode name="marisa_twitch_jump">
			<AudioSource name="@:Sounds/Prefabs/AudioSource_Twitch_Max.prefab"/>
			<AudioClip ClipName="@:Sounds/Twitch/twitch_jump.wav"/>
			<LocalCrouchVolumeScale value="1.0"/>
			<CrouchNoiseScale value="0.5"/>
			<NoiseScale value="1"/>
			<MaxVoices value="30"/>
			<MaxRepeatRate value="0.001"/>
		</SoundDataNode>

		<SoundDataNode name="marisa_twitch_record_scratch">
			<AudioSource name="@:Sounds/Prefabs/AudioSource_Twitch_Loud.prefab"/>
			<AudioClip ClipName="@:Sounds/Twitch/twitch_record_scratch.wav"/>
			<LocalCrouchVolumeScale value="1.0"/>
			<CrouchNoiseScale value="0.5"/>
			<NoiseScale value="1"/>
			<MaxVoices value="30"/>
			<MaxRepeatRate value="0.001"/>
		</SoundDataNode>

	</append>

</Marisa>
