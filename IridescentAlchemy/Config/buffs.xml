<Marisa>

	<append xpath="/buffs">

		<!-- buff ArmorPathFinder -->
		<buff name="buffArmorPathFinder" name_key="buffArmorPathFinderName" description_key="buffArmorPathFinderDesc" showonhud="true" icon="ui_game_symbol_armor_iron" icon_color="255,71,207">
			<stack_type value="replace"/>
			<duration value="0"/>
			<effect_group>
				<passive_effect name="HypothermalResist" operation="base_add" value="10"/>
				<passive_effect name="HyperthermalResist" operation="base_add" value="10"/>
				<passive_effect name="BlockDamage" operation="perc_add" value=".2"/>
				<passive_effect name="HarvestCount" operation="perc_add" value=".2" tags="oreWoodHarvest"/>
				<passive_effect name="LootStage" operation="perc_add" value=".1"/>
			</effect_group>
		</buff>

		<!-- buff ArmorRover -->
		<buff name="buffArmorRover" name_key="buffArmorRoverName" description_key="buffArmorRoverDesc" showonhud="true" icon="ui_game_symbol_armor_iron" icon_color="255,71,207">
			<stack_type value="replace"/>
			<duration value="0"/>
			<effect_group>
				<requirement name="!HoldingItemHasTags" tags="shotgun"/>
					<passive_effect name="DamageModifier" operation="base_add" value="25" tags="head"/>
			</effect_group>
			<effect_group>
				<requirement name="HoldingItemHasTags" tags="shotgun"/>
					<passive_effect name="DamageModifier" operation="base_add" value="2.5" tags="head"/>
			</effect_group>
			<effect_group>
				<requirement name="CVarCompare" cvar="_crouching" operation="Equals" value="1"/>
				<requirement name="CVarCompare" cvar="_notAlerted" operation="GT" value="0" target="other"/>
					<passive_effect name="DamageBonus" operation="base_add" value=".5"/>
					<passive_effect name="EntityDamage" operation="perc_add" value=".5"/>
					<display_value name="dEntityDamageSneak" value=".5"/>
			</effect_group>
			<effect_group>
				<passive_effect name="CrouchSpeed" operation="base_add" value=".2"/>
				<passive_effect name="NoiseMultiplier" operation="perc_add" value="-.15"/>
				<passive_effect name="AttributeLevel" tags="attAgility" operation="base_add" value="1"/>
			</effect_group>
		</buff>

		<!-- buff ArmorGuardian -->
		<buff name="buffArmorGuardian" name_key="buffArmorGuardianName" description_key="buffArmorGuardianDesc" showonhud="true" icon="ui_game_symbol_armor_iron" icon_color="255,71,207">
			<stack_type value="replace"/>
			<duration value="0"/>
			<effect_group>
				<passive_effect name="HealthMax" operation="base_add" value="20"/>
				<passive_effect name="StaminaMax" operation="base_add" value="50"/>
				<passive_effect name="BuffResistance" operation="base_add" value="0.2" tags="buffFatiguedTrigger,buffArmSprainedCHTrigger,buffLegSprainedCHTrigger,buffLaceration,buffInfectionCatch,buffInjuryStunned01CHTrigger,buffInjuryBleedingTwo,buffInjuryBleedingBarbedWire,critResistDisplay"/>
				<passive_effect name="AttributeLevel" tags="attFortitude" operation="base_add" value="1"/>
			</effect_group>
		</buff>

		<!-- buff ArmorInvestigator -->
		<buff name="buffArmorInvestigator" name_key="buffArmorInvestigatorName" description_key="buffArmorInvestigatorDesc" showonhud="true" icon="ui_game_symbol_armor_iron" icon_color="255,71,207">
			<stack_type value="replace"/>
			<duration value="0"/>
			<effect_group>
				<passive_effect name="BarteringSelling" operation="base_add" value=".1"/>
				<passive_effect name="BarteringBuying" operation="base_add" value=".1"/>
				<passive_effect name="PlayerExpGain" operation="perc_add" value=".1"/>
				<passive_effect name="CraftingTime" operation="perc_add" value="-.15"/>
				<passive_effect name="ScrappingTime" operation="perc_add" value="-.8"/>
				<passive_effect name="HarvestCount" operation="perc_add" value=".2" tags="salvageHarvest"/>
				<passive_effect name="AttributeLevel" tags="attIntellect" operation="base_add" value="1"/>
			</effect_group>
		</buff>

		<!-- buff ArmorCombatant -->
		<buff name="buffArmorCombatant" name_key="buffArmorCombatantName" description_key="buffArmorCombatantDesc" showonhud="true" icon="ui_game_symbol_armor_iron" icon_color="255,71,207">
			<stack_type value="replace"/>
			<duration value="0"/>
			<effect_group>
				<passive_effect name="Mobility" operation="perc_add" value=".1"/>
				<passive_effect name="DamageModifier" operation="perc_add" value=".1"/>
				<passive_effect name="AttributeLevel" tags="attPerception" operation="base_add" value="1"/>
			</effect_group>
		</buff>

		<!-- buff ArmorPredator -->
		<buff name="buffArmorPredator" name_key="buffArmorPredatorName" description_key="buffArmorPredatorDesc" showonhud="true" icon="ui_game_symbol_armor_iron" icon_color="255,71,207">
			<stack_type value="replace"/>
			<duration value="0"/>
			<effect_group>
				<passive_effect name="StaminaLoss" operation="perc_add" value="-.1"/>
				<passive_effect name="GeneralDamageResist" operation="base_add" value=".1"/>
				<passive_effect name="AttributeLevel" tags="attStrength" operation="base_add" value="1"/>
			</effect_group>
		</buff>

	</append>

