<Marisa>

    <!-- ** Forest -->
    <csv xpath="/entitygroups/entitygroup[@name='ZombiesForestDowntown']/text()" delim="\n" op="add" >
        EntityBossAnimalDireWolf
    </csv>
    <csv xpath="/entitygroups/entitygroup[@name='ZombiesForestDowntownNight']/text()" delim="\n" op="add" >
        EntityBossAnimalDireWolf
    </csv>
    <csv xpath="/entitygroups/entitygroup[@name='EnemyAnimalsBurntForest']/text()" delim="\n" op="add" >
        EntityBossAnimalDireWolf, 3
    </csv>

    <!-- ** BurntForest -->
    <csv xpath="/entitygroups/entitygroup[@name='ZombiesForestDowntownNight']/text()" delim="\n" op="add" >
        EntityBossAnimalDireWolf
    </csv>
    <csv xpath="/entitygroups/entitygroup[@name='ZombiesBurntForest']/text()" delim="\n" op="add" >
        EntityBossAnimalDireWolf, .2
        EntityBossAnimalBear, .1
    </csv>

    <!-- ** Desert -->
    <csv xpath="/entitygroups/entitygroup[@name='EnemyAnimalsDesert']/text()" delim="\n" op="add" >
        EntityBossAnimalZombieVulture, 20
    </csv>

    <!-- ** Snow -->
    <csv xpath="/entitygroups/entitygroup[@name='SnowZombies']/text()" delim="\n" op="add" >
        EntityBossAnimalDireWolf
        EntityBossAnimalBear
    </csv>
    <csv xpath="/entitygroups/entitygroup[@name='EnemyAnimalsSnow']/text()" delim="\n" op="add" >
        EntityBossAnimalDireWolf, 5
        EntityBossAnimalBear, 5
    </csv>

    <!-- ** Wasteland -->
    <csv xpath="/entitygroups/entitygroup[@name='ZombiesWasteland']/text()" delim="\n" op="add" >
        EntityBossAnimalDireWolf
        EntityBossAnimalBear
        EntityBossAnimalZombieVulture
    </csv>
    <csv xpath="/entitygroups/entitygroup[@name='ZombiesWastelandNight']/text()" delim="\n" op="add" >
        EntityBossAnimalDireWolf
        EntityBossAnimalBear
        EntityBossAnimalZombieVulture
    </csv>
    <csv xpath="/entitygroups/entitygroup[@name='ZombiesWastelandDowntown']/text()" delim="\n" op="add" >
        EntityBossAnimalDireWolf
        EntityBossAnimalBear
        EntityBossAnimalZombieVulture
    </csv>
    <csv xpath="/entitygroups/entitygroup[@name='EnemyAnimalsWasteland']/text()" delim="\n" op="add" >
        EntityBossAnimalDireWolf, 30
        EntityBossAnimalBear, 25
        EntityBossAnimalZombieVulture, 20
    </csv>

</Marisa>
