<Marisa>

	<append xpath="/gameevents">

		<!-- *** resource -->
        <!-- ** resource zombieArlene -->
		<action_sequence name="EventDropResourceZombieArlene">
			<action class="AddItems">
				<property name="added_items" value="resourceZombieArleneRottingFlesh,resourceZombieArleneBone,resourceZombieArleneLeather,resourceZombieArleneScale"/>
				<property name="added_item_counts" value="0-2,0-2,0-2,0-1"/>
			</action>
		</action_sequence>
		<action_sequence name="EventDropResourceExtractZombieArlene">
			<action class="AddItems">
				<property name="added_items" value="resourceZombieArleneExtract"/>
				<property name="added_item_counts" value="1"/>
			</action>
		</action_sequence>
		
	</append>

</Marisa>