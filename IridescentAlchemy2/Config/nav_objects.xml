<Marisa>

	<!--僵尸追踪-->
	<append xpath="/nav_object_classes">

		<nav_object_class name="zombieMarisaAT">
			<property name="requirement_type" value="Tracking"/>
			<map_settings>
				<property name="min_distance" value="0"/>
				<property name="max_distance" value="100"/>
				<property name="has_pulse" value="false"/>
				<property name="sprite_name" value="server_z_never_run"/>
				<property name="color" value="255,0,0,255"/>
			</map_settings>
			<compass_settings>
				<property name="sprite_name" value="ui_game_symbol_zombie"/>
				<property name="min_distance" value="0"/>
				<property name="max_distance" value="1024"/>
				<property name="max_scale_distance" value="32"/>
				<property name="color" value="255,0,0,255"/>
				<property name="has_pulse" value="true"/>
				<property name="icon_clamped" value="false" />
			</compass_settings>
			<onscreen_settings>
				<property name="min_distance" value="0"/>
				<property name="max_distance" value="100"/>
				<property name="max_scale_distance" value="50"/>
				<property name="has_pulse" value="true"/>
				<property name="text_type" value="Distance"/>
				<property name="offset" value="0,1,0" />
				<property name="show_offscreen" value="false" />
				<property name="sprite_name" value="server_z_never_run"/>
				<property name="color" value="255,0,0,255"/>
			</onscreen_settings>
		</nav_object_class>

	</append>

</Marisa>


	